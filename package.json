{
    "name": "claims_app",
    "version": "1.0.0",
    "description": "claims management application API server",
    "main": "server.js",
    "scripts": {
        "start": "nodemon server/server.js --exec babel-node",
        "dev:start": "nodemon server/server.js --exec babel-node",
        "prod:start": "node ./build/server.js",
        "build-server": "babel server -d build",
        "windows:clean": "rmdir /s build && mkdir build",
        "linux:clean": "rm -rf build && mkdir build",
        "windows:build": "npm run windows:clean && npm run build-server",
        "linux:build": "npm run linux:clean && npm run build-server"
    },
    "keywords": [
        "claims",
        "API",
        "server"
    ],
    "engines": {
        "node": ">=v8.0.0"
    },
    "author": "Abinash Patra",
    "license": "ISC",
    "dependencies": {
        "@uirouter/angularjs": "^1.0.4",
        "angular": "^1.6.4",
        "body-parser": "^1.17.2",
        "bootstrap": "^4.0.0-alpha.6",
        "express": "^4.15.3",
        "font-awesome": "^4.7.0",
        "jquery": "^3.2.1",
        "lodash": "^4.17.4",
        "mongoose": "^4.10.6",
        "tether": "^1.4.0"
    },
    "devDependencies": {
        "babel-eslint": "^7.2.3",
        "babel-preset-es2015": "^6.24.1",
        "eslint": "^4.0.0",
        "eslint-plugin-angular": "^2.5.0",
        "eslint-plugin-node": "^5.0.0",
        "nodemon": "^1.11.0",
        "webpack": "^2.6.1"
    }
}